<Window x:Class="Finals_Landlord_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Finals_Landlord_"
        mc:Ignorable="d"
        Title="Landlord Management" Height="266" Width="615" ResizeMode="NoResize" Icon="/Icon.png">
    <Grid>
        <Image Margin="-33,42,359,0" Source="/ProductName.png" Grid.RowSpan="2"/>
        <Label Content="Input Product Code&#xD;&#xA;" HorizontalAlignment="Left" Height="57" Margin="287,33,0,0" VerticalAlignment="Top" Width="257" FontWeight="Bold" FontSize="27" Grid.RowSpan="2"/>
        <TextBox x:Name="Passcode" HorizontalAlignment="Left" Height="50" Margin="264,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="304" FontSize="32" TextChanged="Passcode_TextChanged"/>
        <Button x:Name="Confirm" Content="Enter" HorizontalAlignment="Left" Height="36" Margin="326,164,0,0" VerticalAlignment="Top" Width="179" FontWeight="Bold" FontSize="16" Click="Confirm_Click"/>
    </Grid>
</Window>
